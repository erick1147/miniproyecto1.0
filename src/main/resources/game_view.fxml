<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.stage.Stage?>

<Stage xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.microproyects.controller.GameController">
    <scene>
        <Scene>
            <AnchorPane prefHeight="493.0" prefWidth="762.0">
            <children>
               <Label fx:id="wordLabel" alignment="CENTER" layoutX="290.0" layoutY="167.0" prefHeight="39.0" prefWidth="183.0" text="palabra" textAlignment="CENTER" textFill="#804e4e" />
