               <Button fx:id="validateButton" layoutX="355.0" layoutY="333.0" mnemonicParsing="false" onAction="#validateWord" text="Button" />
               <Label fx:id="levelLabel" layoutX="588.0" layoutY="35.0" prefHeight="17.0" prefWidth="127.0" text="NIVEL ACTUAL" />
               <Label fx:id="timeLabel" alignment="CENTER" layoutX="310.0" layoutY="86.0" prefHeight="72.0" prefWidth="144.0" text="TIEMPO RESTANTE" />
               <Label fx:id="messageLabel" layoutX="365.0" layoutY="238.0" text="Label" />
            </children></AnchorPane>
        </Scene>
    </scene>
</Stage>
